{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1> {{ 'form.events.new'|trans }} </h1>

                {{ form_start(eventosForm) }}
                {{ form_widget(eventosForm) }}

                <button type="submit" class="btn btn-primary">{{ 'form.save'|trans }}</button>
                <a class="btn btn-primary" href="{{ path('eventos_list') }}">{{ 'form.back'|trans }}</a>
                {{ form_end(eventosForm) }}
            </div>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}